<launch>
 <!-- 开启机器人底层相关节点 同时开启导航功能-->
 <include file="$(find robot_bringup)/launch/bringup.launch"/>

 <!-- turn on lidar开启雷达  -->
 <include file="$(find ldlidar_14)/launch/ld14.launch" />

 <!-- 设置需要用于导航的地图  -->
 <arg name="map_file" default="$(find robot_bringup)/map/MAP.yaml"/>
 <node name="map_server_navi" pkg="map_server" type="map_server" args="$(arg map_file)">
 </node>

 <!-- 开启用于导航的自适应蒙特卡洛定位amcl-->
 <include file="$(find robot_bringup)/launch/amcl.launch" />

 <!-- 运行 move_base 节点-->
  <include file="$(find robot_bringup)/launch/teb_local_planner.launch" />
  <!-- <include file="$(find robot_bringup)/launch/dwa_local_planner.launch" /> -->

 <!-- 多点导航> -->
 <node name='send_mark' pkg="robot_bringup" type="send_mark.py">
 </node>

 <node name='node_detection' pkg="robot_bringup" type="node_ping.py">
 	<param name="node_name" type="string" value="/move_base"/>
 </node>

</launch>




